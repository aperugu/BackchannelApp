<h1>Listing comm_votes</h1>

<table>
  <tr>
    <th>User</th>
    <th>Vote</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @comm_votes.each do |comm_vote| %>
  <tr>
    <td><%= comm_vote.user_id %></td>
    <td><%= comm_vote.vote_id %></td>
    <td><%= link_to 'Show', comm_vote %></td>
    <td><%= link_to 'Edit', edit_comm_vote_path(comm_vote) %></td>
